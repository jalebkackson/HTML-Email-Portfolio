    <!-- contact section -->
    <div class="contactSection" id="contactMe">
      <form
        class="contact-form container"
        id="contact-form"
        method="POST"
        action="/send"
        enctype="multipart/form-data"
        autocomplete="off"
      >
        <h1 class="contactH1" data-value="Contact Me" id="hacker">
          Contact Me
        </h1>
        <div class="form-group">
          <input
            type="text"
            class="form-control"
            id="name"
            placeholder="Name"
            name="name"
            required
          />
          <input
            type="email"
            class="form-control"
            id="email"
            placeholder="email"
            name="email"
            required
          />
          <textarea
            type="text"
            class="form-control"
            id="contactMessage"
            rows="3"
            placeholder="Your message"
            name="message"
            required
          ></textarea>
          <div class="submitBtn text-center btn-lg">
            <button type="submit" class="btn btn-outline-info">Submit</button>
          </div>
        </div>
      </form>
